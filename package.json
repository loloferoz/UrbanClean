    "format": "prettier --check \"**/src/**/*.{ts,js,json,css,scss,html}\"",
    "format:fix": "prettier --write \"**/src/**/*.{ts,js,json,css,scss,html}\"",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "localize": "ng extract-i18n --output-path src/locale",
    "palette": "ng generate @angular/material:theme-color"
